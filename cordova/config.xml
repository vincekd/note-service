<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sticklet.workshop" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Sticklet</name>
    <description>
      Sticklet.com android app
    </description>
    <author email="admin@sticklet.com" href="https://www.sticklet.com">
      Vincent Dumas
    </author>
    <content src="index.html" />
    <!-- <plugin name="cordova-plugin-whitelist" spec="1" /> -->
    <access origin="http://192.168.1.107" subdomains="true" />
    <access origin="https://www.sticklet.com" subdomains="true" />
    <allow-navigation href="http://192.168.1.107/*" />
    <allow-navigation href="https://www.sticklet.com/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
